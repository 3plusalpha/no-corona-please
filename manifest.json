{
  "name": "Hide Corona News",
  "version": "0.1",
  "description": "Hide corona related news",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.3.1.min.js", "content.js"]
    }
  ]
}
